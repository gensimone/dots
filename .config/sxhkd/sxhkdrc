XF86AudioRaiseVolume
        pulsemixer --set-volume +$(($(pulsemixer --get-volume | cut -d ' ' -f 1)+10))
XF86AudioLowerVolume
        pulsemixer --set-volume $(($(pulsemixer --get-volume | cut -d ' ' -f 1)-10))
XF86AudioMute
        pulsemixer --toggle-mute
XF86AudioMicMute
        pulsemixer --toggle-mute --id source-1
XF86MonBrightnessUp
	brightnessctl set 10%+
XF86MonBrightnessDown
	brightnessctl set 10%-
alt + i
        pulsemixer --set-volume +$(($(pulsemixer --get-volume | cut -d ' ' -f 1)+10))
alt + u
        pulsemixer --set-volume $(($(pulsemixer --get-volume | cut -d ' ' -f 1)-10))
alt + m
        pulsemixer --toggle-mute
alt + n
        pulsemixer --toggle-mute --id source-1
alt + Escape
	slock
alt + c
        clipmenu
alt + d
        /home/simone/.local/bin/ddocs
alt + a
        /home/simone/.local/bin/dict
