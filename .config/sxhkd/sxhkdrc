XF86AudioRaiseVolume
        pulsemixer --set-volume +$(($(pulsemixer --get-volume | cut -d ' ' -f 1)+5))
XF86AudioLowerVolume
        pulsemixer --set-volume $(($(pulsemixer --get-volume | cut -d ' ' -f 1)-5))
XF86AudioMute
        pulsemixer --toggle-mute
XF86AudioMicMute
        pulsemixer --toggle-mute --id source-1
XF86MonBrightnessUp
	brightnessctl set 10%+
XF86MonBrightnessDown
	brightnessctl set 10%-
alt + i
        pulsemixer --set-volume +$(($(pulsemixer --get-volume | cut -d ' ' -f 1)+5))
alt + u
        pulsemixer --set-volume $(($(pulsemixer --get-volume | cut -d ' ' -f 1)-5))
alt + m
        pulsemixer --toggle-mute
alt + n
        pulsemixer --toggle-mute --id source-1
alt + Escape
	slock
alt + c
        clipmenu
alt + shift + Return
        st
alt + d
        /home/simone/.local/bin/dmenu-docs
alt + shift + d
        /home/simone/.local/bin/dmenu-dict
alt + shift + y
        /home/simone/.local/bin/dmenu-ytdl > /tmp/yt-dlp
alt + shift + k
        /home/simone/.local/bin/dmenu-kill
alt + w
	/home/simone/.local/bin/dmenu-wall
alt + a
        LC_ALL=it_IT.UTF-8 /home/simone/.local/bin/dmenu-tran
alt + p
        dmenu_run
alt + e
        emacsclient -c